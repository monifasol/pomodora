{
  "name": "pomodora",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://monifasol.github.io/pomodora/build",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.49.8",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "start": "sass src/index.scss:public/index.css && react-scripts start",
